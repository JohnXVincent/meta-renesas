From 537a25110c46b06261e46bc00f584517a8b852f6 Mon Sep 17 00:00:00 2001
From: Tien Le <tien.le.xw@renesas.com>
Date: Mon, 1 Mar 2021 17:41:08 +0700
Subject: [PATCH 083/135] arm64: dts: r9a07g044l: Add TSU device nodes

This patch adds TSU device node for r9a07g044l SoC

Signed-off-by: Tien Le <tien.le.xw@renesas.com>
---
 arch/arm64/boot/dts/renesas/r9a07g044l.dtsi | 11 +++++++++++
 1 file changed, 11 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi b/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
index 5941253..8e634d8 100644
--- a/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
+++ b/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
@@ -234,6 +234,17 @@
 			status = "disabled";
 		};
 
+		tsu: serial@10059400 {
+			compatible = "renesas,tsu-r9a07g044l";
+			reg = <0 0x10059400 0 0x400>;
+			clocks = <&cpg CPG_MOD R9A07G044L_CLK_TSU_PCLK>;
+			resets = <&cpg R9A07G044L_CLK_TSU_PCLK>;
+			clock-names = "tsu";
+			thermal-cal = <1>;
+			#thermal-sensor-cells = <0>;
+			power-domains = <&cpg>;
+		};
+
 		dma0: dma-controller@11800000 {
 			compatible = "renesas,rza-dma";
 			reg =   <0 0x11820000 0 0x10000>,
-- 
