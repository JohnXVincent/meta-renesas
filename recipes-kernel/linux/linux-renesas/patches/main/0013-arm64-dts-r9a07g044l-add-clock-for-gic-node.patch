From 9a0a449004147439fa649d39da24285e59e25986 Mon Sep 17 00:00:00 2001
From: Binh Nguyen <binh.nguyen.jz@renesas.com>
Date: Tue, 9 Mar 2021 11:30:06 +0700
Subject: [PATCH 013/135] arm64: dts: r9a07g044l: add clock for gic node

Add clock source control for gic module

Signed-off-by: Binh Nguyen <binh.nguyen.jz@renesas.com>
---
 arch/arm64/boot/dts/renesas/r9a07g044l.dtsi | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi b/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
index afb9490..d0a09f0 100644
--- a/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
+++ b/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
@@ -44,6 +44,8 @@
 			interrupt-controller;
 			reg = <0x0 0x11900000 0 0x40000>,
 			      <0x0 0x11940000 0 0x60000>;
+			clocks = <&cpg CPG_MOD R9A07G044L_CLK_GIC600>;
+			clock-names = "gic6000";
 			interrupts = <GIC_PPI 9
 				(GIC_CPU_MASK_SIMPLE(2) | IRQ_TYPE_LEVEL_LOW)>;
 		};
-- 
