From cf774470861f2cfa0036764611eaa4e11f0bf5d3 Mon Sep 17 00:00:00 2001
From: hoahuynh <hoa.huynh.ud@renesas.com>
Date: Mon, 1 Mar 2021 19:11:19 +0700
Subject: [PATCH 111/135] arm64: dts: renesas: r9a07g044l: Add HSUSB device
 node

This patch adds hsusb device nodes for r9a07g044l SoC.

Signed-off-by: hoahuynh <hoa.huynh.ud@renesas.com>
---
 arch/arm64/boot/dts/renesas/r9a07g044l.dtsi | 14 ++++++++++++++
 1 file changed, 14 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi b/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
index 3de231e..4678b79 100644
--- a/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
+++ b/arch/arm64/boot/dts/renesas/r9a07g044l.dtsi
@@ -408,6 +408,20 @@
 			status = "disabled";
 		};
 
+		hsusb: usb@11c60000 {
+			compatible = "renesas,usbhs-r9a07g044l",
+				     "renesas,g2l-usbhs";
+			reg = <0 0x11c60000 0 0x724>;
+			interrupts = <GIC_SPI 100 IRQ_TYPE_EDGE_RISING>;
+			clocks = <&cpg CPG_MOD R9A07G044L_CLK_USB0>;
+			resets = <&cpg R9A07G044L_CLK_USB0>;
+			renesas,buswait = <7>;
+			phys = <&usbtest>, <&usb2_phy0>;
+			phy-names = "usb";
+			power-domains = <&cpg>;
+			status = "disabled";
+		};
+
 		usb2_phy0: usb-phy@11c50200 {
 			compatible = "renesas,usb2-phy-r9a07g044l",
 				     "renesas,rcar-gen3-usb2-phy";
-- 
