From 66abbebf8baf07d38a1e89f0afae3795bc831bf5 Mon Sep 17 00:00:00 2001
From: Quynh Nguyen <quynhnguyen.xb@renesas.com>
Date: Thu, 15 Oct 2020 13:56:31 +0700
Subject: [PATCH 309/475] ARM: dts: r8a7742-iwg21d-q7: Add power-supply support
 fan

Signed-off-by: Tung Vo <tung.vo.eb@rvc.renesas.com>
Signed-off-by: Quynh Nguyen <quynhnguyen.xb@renesas.com>
---
 arch/arm/boot/dts/r8a7742-iwg21d-q7.dts | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts b/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts
index 2f322ad..f69eaf2 100644
--- a/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts
+++ b/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts
@@ -38,6 +38,7 @@
 		pwms = <&tpu 1 5000000 GPIO_ACTIVE_HIGH>;
 		brightness-levels = <0 4 8 16 32 64 128 255>;
 		default-brightness-level = <7>;
+		power-supply = <&vcc_fan>;
 		pinctrl-0 = <&fan_pins>;
 		pinctrl-names = "default";
 	};
@@ -50,6 +51,14 @@
 		regulator-always-on;
 	};
 
+	vcc_fan: regulator@6 {
+		compatible = "regulator-fixed";
+		regulator-name = "fan vcc";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		regulator-always-on;
+	};
+
 	vcc_sdhi2: regulator-vcc-sdhi2 {
 		compatible = "regulator-fixed";
 
-- 
2.7.4

