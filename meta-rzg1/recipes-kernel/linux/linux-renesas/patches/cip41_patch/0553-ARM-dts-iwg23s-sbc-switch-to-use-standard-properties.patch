From 9c792292efc97f640205bcaa62eb9339f82b2b46 Mon Sep 17 00:00:00 2001
From: Hao Bui <hao.bui.yg@renesas.com>
Date: Wed, 22 Apr 2020 08:11:33 +0700
Subject: [PATCH 553/557] ARM: dts: iwg23s-sbc: switch to use standard
 properties of PFC node

Signed-off-by: Hao Bui <hao.bui.yg@renesas.com>
---
 arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts | 80 +++++++++++++++----------------
 1 file changed, 40 insertions(+), 40 deletions(-)

diff --git a/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts b/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
index 9061743..a0a2d93 100644
--- a/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
+++ b/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
@@ -136,18 +136,18 @@
 
 &pfc {
 	avb_pins: avb {
-		renesas,groups = "avb_mdio", "avb_gmii_tx_rx", "avb_phy_int";
-		renesas,function = "avb";
+		groups = "avb_mdio", "avb_gmii_tx_rx", "avb_phy_int";
+		function = "avb";
 	};
 
 	phy0_pins: phy0 {
-		renesas,groups = "intc_irq8";
-		renesas,function = "intc";
+		groups = "intc_irq8";
+		function = "intc";
 	};
 
 	i2c3_pins: i2c3 {
-		renesas,groups = "i2c3_c";
-		renesas,function = "i2c3";
+		groups = "i2c3_c";
+		function = "i2c3";
 	};
 
 	mmc_pins_uhs: mmc_uhs {
@@ -157,8 +157,8 @@
 	};
 
 	qspi0_pins: qspi0 {
-		renesas,groups = "qspi0_ctrl", "qspi0_data2";
-		renesas,function = "qspi0";
+		groups = "qspi0_ctrl", "qspi0_data2";
+		function = "qspi0";
 	};
 
 	sdhi2_pins: sd2 {
@@ -174,84 +174,84 @@
 	};
 
 	usb0_pins: usb0 {
-		renesas,groups = "usb0";
-		renesas,function = "usb0";
+		groups = "usb0";
+		function = "usb0";
 	};
 
 	usb1_pins: usb1 {
-		renesas,groups = "usb1";
-		renesas,function = "usb1";
+		groups = "usb1";
+		function = "usb1";
 	};
 
 	scif1_pins: serial1 {
-		renesas,groups = "scif1_data_b";
-		renesas,function = "scif1";
+		groups = "scif1_data_b";
+		function = "scif1";
 	};
 
 	scif2_pins: serial2 {
-		renesas,groups = "scif2_data_b";
-		renesas,function = "scif2";
+		groups = "scif2_data_b";
+		function = "scif2";
 	};
 
 	scif4_pins: serial3 {
-		renesas,groups = "scif4_data_b";
-		renesas,function = "scif4";
+		groups = "scif4_data_b";
+		function = "scif4";
 	};
 
 	scif5_pins: serial4 {
-		renesas,groups = "scif5_data_b";
-		renesas,function = "scif5";
+		groups = "scif5_data_b";
+		function = "scif5";
 	};
 
 	hscif1_pins: serial5 {
-		renesas,groups = "hscif1_data", "hscif1_ctrl_a";
-		renesas,function = "hscif1";
+		groups = "hscif1_data", "hscif1_ctrl_a";
+		function = "hscif1";
 	};
 
 	hscif2_pins: serial6 {
-		renesas,groups = "hscif2_data", "hscif2_clk", "hscif2_ctrl";
-		renesas,function = "hscif2";
+		groups = "hscif2_data", "hscif2_clk", "hscif2_ctrl";
+		function = "hscif2";
 	};
 
 	can0_pins: can0 {
-		renesas,groups = "can0_data_a";
-		renesas,function = "can0";
+		groups = "can0_data_a";
+		function = "can0";
 	};
 
 	i2c4_pins: i2c4 {
-		renesas,groups = "i2c4_e";
-		renesas,function = "i2c4";
+		groups = "i2c4_e";
+		function = "i2c4";
 	};
 
 	msiof2_pins: spi4 {
-		renesas,groups = "msiof2_clk_a", "msiof2_sync_a", "msiof2_ss1_a", "msiof2_rx_a", "msiof2_tx_a";
-		renesas,function = "msiof2";
+		groups = "msiof2_clk_a", "msiof2_sync_a", "msiof2_ss1_a", "msiof2_rx_a", "msiof2_tx_a";
+		function = "msiof2";
 	};
 
 	du0_pins: du0 {
-		renesas,groups = "du0_rgb888", "du0_clk0_out", "du0_sync",
+		groups = "du0_rgb888", "du0_clk0_out", "du0_sync",
 						"du0_cde", "du0_disp";
-		renesas,function = "du0";
+		function = "du0";
 	};
 
 	vin0_pins: vin0 {
-		renesas,groups = "vin0_data8", "vin0_sync", "vin0_clkenb", "vin0_clk";
-		renesas,function = "vin0";
+		groups = "vin0_data8", "vin0_sync", "vin0_clkenb", "vin0_clk";
+		function = "vin0";
 	};
 
 	pwm0_pins: pwm0 {
-		renesas,groups = "pwm0_c";
-		renesas,function = "pwm0";
+		groups = "pwm0_c";
+		function = "pwm0";
 	};
 
 	pwm2_pins: pwm2 {
-		renesas,groups = "pwm2_d";
-		renesas,function = "pwm2";
+		groups = "pwm2_d";
+		function = "pwm2";
 	};
 
 	pwm6_pins: pwm6 {
-		renesas,groups = "pwm6_c";
-		renesas,function = "pwm6";
+		groups = "pwm6_c";
+		function = "pwm6";
 	};
 };
 
