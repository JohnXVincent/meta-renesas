+++ b/arch/arm/boot/dts/r8a77470-iwg23s-sbc.dts
@@ -174,6 +174,11 @@
 		function = "i2c3";
 	};
 
+	i2c4_pins: i2c4 {
+		groups = "i2c4_e";
+		function = "i2c4";
+	};
+
 	mmc_pins_uhs: mmc_uhs {
 		groups = "mmc_data8", "mmc_ctrl";
 		function = "mmc";
