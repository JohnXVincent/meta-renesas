From a156997b5fba5240e1ac7d6416a5754e90f05a61 Mon Sep 17 00:00:00 2001
From: Quynh Nguyen <quynhnguyen.xb@renesas.com>
Date: Thu, 15 Oct 2020 17:19:54 +0700
Subject: [PATCH 424/442] ARM: dts: r8a7742-iwg21d-q7: Add VBUS and PWEN gpio
 for USB OTG

Signed-off-by: Quynh Nguyen <quynhnguyen.xb@renesas.com>
---
 arch/arm/boot/dts/r8a7742-iwg21d-q7.dts | 5 +++++
 1 file changed, 5 insertions(+)

diff --git a/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts b/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts
index 53df357..ccd2131 100644
--- a/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts
+++ b/arch/arm/boot/dts/r8a7742-iwg21d-q7.dts
@@ -502,6 +502,11 @@
 };
 
 &usbphy {
+	renesas,id-gpio = <&gpio5 20 GPIO_ACTIVE_LOW>;
+	renesas,vbus-gpio = <&gpio5 19 GPIO_ACTIVE_LOW>;
+	renesas,vbus-pwr-gpio = <&gpio5 18 GPIO_ACTIVE_LOW>;
+	renesas,wakeup;
+
 	status = "okay";
 };
 
