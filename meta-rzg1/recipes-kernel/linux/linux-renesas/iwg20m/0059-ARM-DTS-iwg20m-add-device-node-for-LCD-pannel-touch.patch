From 0f188b44ca17acde36b3e382ae87e31269f6e7a4 Mon Sep 17 00:00:00 2001
From: Binh Nguyen <binh.nguyen.uw@rvc.renesas.com>
Date: Fri, 29 Sep 2017 02:19:34 +0700
Subject: [PATCH 3/3] ARM: DTS: iwg20m: add device node for LCD pannel touch

Signed-off-by: Binh Nguyen <binh.nguyen.uw@rvc.renesas.com>
---
 arch/arm/boot/dts/r8a7743-iwg20m.dts | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/arch/arm/boot/dts/r8a7743-iwg20m.dts b/arch/arm/boot/dts/r8a7743-iwg20m.dts
index 2afd4902..f0b81b0 100644
--- a/arch/arm/boot/dts/r8a7743-iwg20m.dts
+++ b/arch/arm/boot/dts/r8a7743-iwg20m.dts
@@ -432,6 +432,14 @@
 		VDDD-supply = <&reg_0p0v>;
 		clocks = <&x9_clk 1>;
 	};
+
+	ft5x06@38 {
+		compatible = "edt,edt-ft5406";
+		reg = <0x38>;
+		interrupt-parent = <&gpio2>;
+		interrupts = <12 IRQ_TYPE_EDGE_FALLING>;
+		reset-gpios = <&gpio2 13 GPIO_ACTIVE_LOW>;
+	};
 };
 
 &i2c5 {
