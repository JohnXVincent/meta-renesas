+        gpio_request(GPIO_FN_AVB_RXD2, NULL);
+        gpio_request(GPIO_FN_AVB_RXD3, NULL);
+        gpio_request(GPIO_FN_AVB_RXD4, NULL);
+        gpio_request(GPIO_FN_AVB_RXD5, NULL);
